<?xml version="1.0" encoding="utf-8" standalone="no"?>
<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="@dimen/activity_advertisement_cell_advertisement_cell_height">

	<android.support.constraint.ConstraintLayout
		android:id="@+id/bg"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:layout_marginTop="@dimen/activity_advertisement_cell_bg_margin_top"
		android:background="@color/activity_advertisement_cell_bg_background_color"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="30dp"/>

	<TextView
		android:id="@+id/hull_tattoos_anew_label"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="@dimen/activity_advertisement_cell_hull_tattoos_anew_label_margin_start"
		android:layout_marginTop="@dimen/activity_advertisement_cell_hull_tattoos_anew_label_margin_top"
		android:fontFamily="@font/font_lato_regular"
		android:gravity="left"
		android:text="@string/activity_advertisement_cell_hull_tattoos_anew_label_text"
		android:textColor="@color/activity_advertisement_cell_hull_tattoos_anew_label_text_color"
		android:textSize="@dimen/activity_advertisement_cell_hull_tattoos_anew_label_text_size"
		app:layout_constraintLeft_toRightOf="@+id/bg_event_temp_iv"
		app:layout_constraintTop_toBottomOf="@+id/spray_and_pray_label"
		tools:layout_editor_absoluteX="108dp"
		tools:layout_editor_absoluteY="64dp"/>

	<TextView
		android:id="@+id/sponsored_content_label"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="@dimen/activity_advertisement_cell_sponsored_content_label_margin_start"
		android:layout_marginTop="@dimen/activity_advertisement_cell_sponsored_content_label_margin_top"
		android:alpha="0.400000005960464"
		android:fontFamily="@font/font_lato_regular"
		android:gravity="left"
		android:text="@string/activity_advertisement_cell_sponsored_content_label_text"
		android:textColor="@color/activity_advertisement_cell_sponsored_content_label_text_color"
		android:textSize="@dimen/activity_advertisement_cell_sponsored_content_label_text_size"
		app:layout_constraintLeft_toRightOf="@+id/bg_event_temp_iv"
		app:layout_constraintTop_toBottomOf="@+id/hull_tattoos_anew_label"
		tools:layout_editor_absoluteX="108dp"
		tools:layout_editor_absoluteY="89dp"/>

	<TextView
		android:id="@+id/spray_and_pray_label"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="@dimen/activity_advertisement_cell_spray_and_pray_label_margin_start"
		android:layout_marginTop="@dimen/activity_advertisement_cell_spray_and_pray_label_margin_top"
		android:fontFamily="@font/font_lato_bold"
		android:gravity="left"
		android:text="@string/activity_advertisement_cell_spray_and_pray_label_text"
		android:textColor="@color/activity_advertisement_cell_spray_and_pray_label_text_color"
		android:textSize="@dimen/activity_advertisement_cell_spray_and_pray_label_text_size"
		app:layout_constraintLeft_toRightOf="@+id/bg_event_temp_iv"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="108dp"
		tools:layout_editor_absoluteY="42dp"/>

	<ImageView
		android:id="@+id/bg_event_temp_iv"
		android:layout_width="@dimen/activity_advertisement_cell_bg_event_temp_iv_width"
		android:layout_height="0dp"
		android:layout_marginTop="@dimen/activity_advertisement_cell_bg_event_temp_iv_margin_top"
		android:scaleType="fitXY"
		android:src="@drawable/bg_event_temp_2"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="30dp"/>

</android.support.constraint.ConstraintLayout>
