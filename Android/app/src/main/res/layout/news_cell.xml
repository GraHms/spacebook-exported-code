<?xml version="1.0" encoding="utf-8" standalone="no"?>
<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="@dimen/activity_news_cell_news_cell_height">

	<android.support.constraint.ConstraintLayout
		android:id="@+id/bg_white"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:layout_marginTop="@dimen/activity_news_cell_bg_white_margin_top"
		android:background="@color/activity_news_cell_bg_white_background_color"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="30dp"/>

	<android.support.constraint.ConstraintLayout
		android:id="@+id/view"
		android:layout_width="0dp"
		android:layout_height="@dimen/activity_news_cell_view_height"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="360dp">

		<android.support.constraint.ConstraintLayout
			android:id="@+id/bg_gray"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:background="@color/activity_news_cell_bg_gray_background_color"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="0dp"/>

		<TextView
			android:id="@+id/spacewoman185k_li_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/activity_news_cell_spacewoman185k_li_label_margin_start"
			android:alpha="0.400000005960464"
			android:fontFamily="@font/font_lato_regular"
			android:gravity="left"
			android:text="@string/activity_news_cell_spacewoman185k_li_label_text"
			android:textColor="@color/activity_news_cell_spacewoman185k_li_label_text_color"
			android:textSize="@dimen/activity_news_cell_spacewoman185k_li_label_text_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toRightOf="@+id/icon_like_iv"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="44dp"
			tools:layout_editor_absoluteY="20dp"/>

		<TextView
			android:id="@+id/k_comments_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginRight="@dimen/activity_news_cell_k_comments_label_margin_end"
			android:alpha="0.400000005960464"
			android:fontFamily="@font/font_lato_regular"
			android:gravity="left"
			android:text="@string/activity_news_cell_k_comments_label_text"
			android:textColor="@color/activity_news_cell_k_comments_label_text_color"
			android:textSize="@dimen/activity_news_cell_k_comments_label_text_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="277dp"
			tools:layout_editor_absoluteY="20dp"/>

		<ImageView
			android:id="@+id/icon_like_iv"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/activity_news_cell_icon_like_iv_margin_start"
			android:scaleType="fitXY"
			android:src="@drawable/icon_like"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="20dp"
			tools:layout_editor_absoluteY="20dp"/>

		<ImageView
			android:id="@+id/icon_comment_iv"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginRight="@dimen/activity_news_cell_icon_comment_iv_margin_end"
			android:scaleType="fitXY"
			android:src="@drawable/icon_comment"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintRight_toLeftOf="@+id/k_comments_label"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="253dp"
			tools:layout_editor_absoluteY="20dp"/>

	</android.support.constraint.ConstraintLayout>

	<ImageView
		android:id="@+id/bg_news_temp_iv"
		android:layout_width="0dp"
		android:layout_height="@dimen/activity_news_cell_bg_news_temp_iv_height"
		android:layout_marginTop="@dimen/activity_news_cell_bg_news_temp_iv_margin_top"
		android:scaleType="fitXY"
		android:src="@drawable/bg_news_temp_2"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="80dp"/>

	<android.support.constraint.ConstraintLayout
		android:id="@+id/view_two"
		android:layout_width="0dp"
		android:layout_height="@dimen/activity_news_cell_view_two_height"
		android:layout_marginLeft="@dimen/activity_news_cell_view_two_margin_start"
		android:layout_marginRight="@dimen/activity_news_cell_view_two_margin_end"
		android:layout_marginTop="@dimen/activity_news_cell_view_two_margin_top"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="10dp"
		tools:layout_editor_absoluteY="15dp">

		<TextView
			android:id="@+id/spacecrunch_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/activity_news_cell_spacecrunch_label_margin_start"
			android:layout_marginTop="@dimen/activity_news_cell_spacecrunch_label_margin_top"
			android:fontFamily="@font/font_lato_bold"
			android:gravity="left"
			android:text="@string/activity_news_cell_spacecrunch_label_text"
			android:textColor="@color/activity_news_cell_spacecrunch_label_text_color"
			android:textSize="@dimen/activity_news_cell_spacecrunch_label_text_size"
			app:layout_constraintLeft_toRightOf="@+id/icon_avatar_iv"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="89dp"
			tools:layout_editor_absoluteY="20dp"/>

		<TextView
			android:id="@+id/today145_pm_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/activity_news_cell_today145_pm_label_margin_start"
			android:layout_marginTop="@dimen/activity_news_cell_today145_pm_label_margin_top"
			android:alpha="0.400000005960464"
			android:fontFamily="@font/font_lato_regular"
			android:gravity="left"
			android:text="@string/activity_news_cell_today145_pm_label_text"
			android:textColor="@color/activity_news_cell_today145_pm_label_text_color"
			android:textSize="@dimen/activity_news_cell_today145_pm_label_text_size"
			app:layout_constraintLeft_toRightOf="@+id/icon_avatar_iv"
			app:layout_constraintTop_toBottomOf="@+id/spacecrunch_label"
			tools:layout_editor_absoluteX="89dp"
			tools:layout_editor_absoluteY="42dp"/>

		<ImageView
			android:id="@+id/icon_avatar_iv"
			android:layout_width="@dimen/activity_news_cell_icon_avatar_iv_width"
			android:layout_height="0dp"
			android:scaleType="fitXY"
			android:src="@drawable/icon_avatar"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="0dp"/>

	</android.support.constraint.ConstraintLayout>

	<TextView
		android:id="@+id/melon_husk_launches_label"
		android:layout_width="wrap_content"
		android:layout_height="0dp"
		android:layout_marginBottom="@dimen/activity_news_cell_melon_husk_launches_label_margin_bottom"
		android:layout_marginLeft="@dimen/activity_news_cell_melon_husk_launches_label_margin_start"
		android:layout_marginTop="@dimen/activity_news_cell_melon_husk_launches_label_margin_top"
		android:fontFamily="@font/font_lato_regular"
		android:gravity="left"
		android:text="@string/activity_news_cell_melon_husk_launches_label_text"
		android:textColor="@color/activity_news_cell_melon_husk_launches_label_text_color"
		android:textSize="@dimen/activity_news_cell_melon_husk_launches_label_text_size"
		app:layout_constraintBottom_toTopOf="@+id/view"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/bg_news_temp_iv"
		tools:layout_editor_absoluteX="20dp"
		tools:layout_editor_absoluteY="297dp"/>

</android.support.constraint.ConstraintLayout>
