<?xml version="1.0" encoding="utf-8" standalone="no"?>
<android.support.constraint.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="@dimen/activity_timelinevc_news_cell_size">

	<android.support.constraint.ConstraintLayout
		android:id="@+id/bg_white"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:layout_marginTop="@dimen/activity_timelinevc_bg_white_size"
		android:background="#FFFFFF"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="25dp"/>

	<android.support.constraint.ConstraintLayout
		android:id="@+id/view"
		android:layout_width="0dp"
		android:layout_height="@dimen/activity_timelinevc_view_size"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="307dp">

		<android.support.constraint.ConstraintLayout
			android:id="@+id/bg_gray"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:background="#FAFAFA"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="0dp"/>

		<TextView
			android:id="@+id/spacewoman185k_li_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/activity_timelinevc_spacewoman185k_li_label_size1"
			android:alpha="0.400000005960464"
			android:gravity="left"
			android:text="@string/activity_timelinevc_spacewoman185k_li_label_text"
			android:textColor="@color/activity_timelinevc_spacewoman185k_li_label_background_color"
			android:textSize="@dimen/activity_timelinevc_spacewoman185k_li_label_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toRightOf="@+id/icon_like_iv"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="37dp"
			tools:layout_editor_absoluteY="17dp"/>

		<TextView
			android:id="@+id/k_comments_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginRight="@dimen/activity_timelinevc_k_comments_label_size1"
			android:alpha="0.400000005960464"
			android:gravity="left"
			android:text="@string/activity_timelinevc_k_comments_label_text"
			android:textColor="@color/activity_timelinevc_k_comments_label_background_color"
			android:textSize="@dimen/activity_timelinevc_k_comments_label_size"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="236dp"
			tools:layout_editor_absoluteY="17dp"/>

		<ImageView
			android:id="@+id/icon_like_iv"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/activity_timelinevc_icon_like_iv_size"
			android:scaleType="fitXY"
			android:src="@drawable/icon_like"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="17dp"
			tools:layout_editor_absoluteY="17dp"/>

		<ImageView
			android:id="@+id/icon_comment_iv"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginRight="@dimen/activity_timelinevc_icon_comment_iv_size"
			android:scaleType="fitXY"
			android:src="@drawable/icon_comment"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintRight_toLeftOf="@+id/k_comments_label"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="215dp"
			tools:layout_editor_absoluteY="17dp"/>

	</android.support.constraint.ConstraintLayout>

	<ImageView
		android:id="@+id/bg_news_temp_iv"
		android:layout_width="0dp"
		android:layout_height="@dimen/activity_timelinevc_bg_news_temp_iv_size"
		android:layout_marginTop="@dimen/activity_timelinevc_bg_news_temp_iv_size1"
		android:scaleType="fitXY"
		android:src="@drawable/bg_news_temp"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="0dp"
		tools:layout_editor_absoluteY="68dp"/>

	<android.support.constraint.ConstraintLayout
		android:id="@+id/view_two"
		android:layout_width="0dp"
		android:layout_height="@dimen/activity_timelinevc_view_two_size"
		android:layout_marginLeft="@dimen/activity_timelinevc_view_two_size2"
		android:layout_marginRight="@dimen/activity_timelinevc_view_two_size3"
		android:layout_marginTop="@dimen/activity_timelinevc_view_two_size1"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		tools:layout_editor_absoluteX="8dp"
		tools:layout_editor_absoluteY="12dp">

		<TextView
			android:id="@+id/spacecrunch_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/activity_timelinevc_spacecrunch_label_size2"
			android:layout_marginTop="@dimen/activity_timelinevc_spacecrunch_label_size1"
			android:gravity="left"
			android:text="@string/activity_timelinevc_spacecrunch_label_text"
			android:textColor="@color/activity_timelinevc_spacecrunch_label_background_color"
			android:textSize="@dimen/activity_timelinevc_spacecrunch_label_size"
			app:layout_constraintLeft_toRightOf="@+id/icon_avatar_iv"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="75dp"
			tools:layout_editor_absoluteY="17dp"/>

		<TextView
			android:id="@+id/today145_pm_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/activity_timelinevc_today145_pm_label_size2"
			android:layout_marginTop="@dimen/activity_timelinevc_today145_pm_label_size1"
			android:alpha="0.400000005960464"
			android:gravity="left"
			android:text="@string/activity_timelinevc_today145_pm_label_text"
			android:textColor="@color/activity_timelinevc_today145_pm_label_background_color"
			android:textSize="@dimen/activity_timelinevc_today145_pm_label_size"
			app:layout_constraintLeft_toRightOf="@+id/icon_avatar_iv"
			app:layout_constraintTop_toBottomOf="@+id/spacecrunch_label"
			tools:layout_editor_absoluteX="75dp"
			tools:layout_editor_absoluteY="35dp"/>

		<ImageView
			android:id="@+id/icon_avatar_iv"
			android:layout_width="@dimen/activity_timelinevc_icon_avatar_iv_size"
			android:layout_height="0dp"
			android:scaleType="fitXY"
			android:src="@drawable/icon_avatar"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			tools:layout_editor_absoluteX="0dp"
			tools:layout_editor_absoluteY="0dp"/>

	</android.support.constraint.ConstraintLayout>

	<TextView
		android:id="@+id/melon_husk_launches_label"
		android:layout_width="wrap_content"
		android:layout_height="0dp"
		android:layout_marginBottom="@dimen/activity_timelinevc_melon_husk_launches_label_size1"
		android:layout_marginLeft="@dimen/activity_timelinevc_melon_husk_launches_label_size3"
		android:layout_marginTop="@dimen/activity_timelinevc_melon_husk_launches_label_size2"
		android:gravity="left"
		android:text="@string/activity_timelinevc_melon_husk_launches_label_text"
		android:textColor="@color/activity_timelinevc_melon_husk_launches_label_background_color"
		android:textSize="@dimen/activity_timelinevc_melon_husk_launches_label_size"
		app:layout_constraintBottom_toTopOf="@+id/view"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/bg_news_temp_iv"
		tools:layout_editor_absoluteX="17dp"
		tools:layout_editor_absoluteY="253dp"/>

</android.support.constraint.ConstraintLayout>
